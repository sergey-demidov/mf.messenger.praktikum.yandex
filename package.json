{
  "name": "mf.messenger.praktikum.yandex",
  "version": "0.1.0",
  "description": "just a simple kind of chat",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ttsc -w & node server & node-sass src/css/main.scss -wo static/css & browser-sync start --proxy 'localhost:3000' --files 'static'",
    "serve": "node server",
    "css": "node-sass src/css/main.scss -o static/css",
    "lint": "eslint --ext .ts src",
    "push": "test `git status --porcelain | wc -l` -eq 0 && npm run lint && git push || echo commit and lint first",
    "deploy": "test `git status --porcelain | wc -l` -eq 0 && git rebase main deploy && git push && git checkout main || echo commit first"
  },
