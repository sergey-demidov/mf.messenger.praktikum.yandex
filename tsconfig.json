{
  "compilerOptions": {
    "rootDir": "src",
    "target": "es6",
    "strict": true,
    "declaration": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "lib": ["dom", "es6", "dom.iterable"],
    "outDir": "static"
  }
}
